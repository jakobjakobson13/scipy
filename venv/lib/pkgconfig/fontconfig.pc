prefix=/workspaces/scipy/venv
includedir=${prefix}/include
libdir=${prefix}/lib

sysconfdir=/workspaces/scipy/venv/etc
localstatedir=/workspaces/scipy/venv/var
confdir=/workspaces/scipy/venv/etc/fonts
cachedir=/workspaces/scipy/venv/var/cache/fontconfig

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.14.2
Requires: freetype2 >=  21.0.15
Requires.private: expat
Libs: -L${libdir} -lfontconfig
Libs.private: -pthread
Cflags: -I${includedir}
